{
    "name": "woohoolabs/yin-middleware",
    "description": "Woohoo Labs. Yin Middlewares",
    "type": "library",
    "keywords": ["Woohoo Labs.", "Yin", "Middleware", "HATEOAS", "JSON API", "PSR-7"],
    "homepage": "http://yin.woohoolabs.com",
    "license": "MIT",
    "authors": [
        {
            "name":"Máté Kocsis",
            "email":"kocsismate@woohoolabs.com"
        }
    ],
    "support": {
        "issues": "https://github.com/woohoolabs/yin-middleware/issues",
        "source": "https://github.com/woohoolabs/yin-middleware"
    },
    "require": {
        "php":"^5.5.0||^7.0.0",
        "psr/http-message-implementation": "^1.0.0",
        "woohoolabs/yin": "dev-master as 0.11.0"
    },
    "require-dev": {
        "container-interop/container-interop": "^1.0.0",
        "justinrainbow/json-schema": "^2.0.0",
        "phpunit/phpunit": "^4.4.0||^5.0.0",
        "seld/jsonlint": "^1.4.0",
        "squizlabs/php_codesniffer": "^2.3.1",
        "zendframework/zend-diactoros": "^1.3.0"
    },
    "suggest": {
        "container-interop/container-interop": "Allows to use the JsonApiDispatcher",
        "justinrainbow/json-schema": "Allows to validate the request and response body against the JSON API schema",
        "seld/jsonlint": "Allows to lint the request and response body"
    },
    "autoload": {
        "psr-4": {
            "WoohooLabs\\YinMiddleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WoohooLabsTest\\YinMiddleware\\": "tests/"
        }
    },
    "replace": {
        "woohoolabs/yin-middlewares": "self.version"
    }
}
